<?xml version="1.0" encoding="utf-8"?>
<flow start-state="startViewUser" xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
       http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
       
       <action-state id="startViewUser">
       <on-entry>
       <evaluate expression="patientService.searchForPatients()" result="flowScope.allPatient">
       </evaluate>
       </on-entry>
       
       
       
       </action-state>
       
</flow>