<?xml version="1.0" encoding="utf-8"?>
<flow start-state="startAddUser" xmlns="http://www.springframework.org/schema/webflow"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/webflow
       http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

       <view-state id="startAddUser" view="Menu2/Add/step1">
               <transition  on="step2" to="step2" />
       </view-state>
       <view-state id="step2" view="Menu2/Add/step2">
       		<transition on="done" to="done"/>
       	</view-state>
       <end-state id="done"></end-state>
</flow>
